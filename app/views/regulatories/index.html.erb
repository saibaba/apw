<h1>Listing regulatories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Share</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @regulatories.each do |regulatory| %>
      <tr>
        <td><%= regulatory.name %></td>
        <td><%= regulatory.share %></td>
        <td><%= link_to 'Show', regulatory %></td>
        <td><%= link_to 'Edit', edit_regulatory_path(regulatory) %></td>
        <td><%= link_to 'Destroy', regulatory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Regulatory', new_regulatory_path %>
